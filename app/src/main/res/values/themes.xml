<resources>

    <style name="Stryker" parent="Theme.Material3.Light.NoActionBar">
        <item name="colorPrimary">@color/blue1</item>
        <item name="colorOnPrimary">@color/white</item>
        <item name="colorPrimaryContainer">@color/blue2</item>
        <item name="colorOnPrimaryContainer">@color/blue3</item>
        <item name="colorSecondary">@color/grey1</item>
        <item name="colorOnSecondary">@color/white</item>
        <item name="colorSecondaryContainer">@color/grey2</item>
        <item name="colorOnSecondaryContainer">@color/grey3</item>
        <item name="colorTertiary">@color/blue4</item>
        <item name="colorOnTertiary">@color/white</item>
        <item name="colorTertiaryContainer">@color/blue5</item>
        <item name="colorOnTertiaryContainer">@color/blue6</item>
        <item name="colorError">@color/red1</item>
        <item name="colorErrorContainer">@color/white</item>
        <item name="colorOnError">@color/white</item>
        <item name="colorOnErrorContainer">@color/red3</item>
        <item name="android:colorBackground">@color/white2</item>
        <item name="colorOnBackground">@color/grey4</item>
        <item name="colorSurface">@color/white2</item>
        <item name="colorOnSurface">@color/grey4</item>
        <item name="colorSurfaceVariant">@color/grey6</item>
        <item name="colorOnSurfaceVariant">@color/grey6</item>
        <item name="colorOutline">@color/grey7</item>
        <item name="colorOnSurfaceInverse">@color/grey8</item>
        <item name="colorSurfaceInverse">@color/grey9</item>
        <item name="colorPrimaryInverse">@color/blue7</item>
        <item name="android:textColor">@color/grey4</item>
        <item name="android:statusBarColor">#FFFFFF</item>
        <item name="android:navigationBarColor">#FFFFFF</item>
        <item name="colorSurfaceContainerHigh">#FFFFFF</item>


    </style>

    <style name="CustomDialogTheme" parent="Theme.AppCompat.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.5</item>
    </style>

    <style name="Stryker.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Stryker.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Stryker.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>